@Html.Partial("_QuestionModalv2")
@section Scripts {
    <script>
    $(function () {
        if (detectMobileDevice() == 1) {
            //$("#includedContentMenuMobile").load("/arena20/includes/menu-logged-mobile.html");
            //$("#includedContentMenuRankingMobile").load("menu-my-matches-mobile.html");
            hidden_show_ObjectMobile("footerMobile", "block");
        }
    });


    $(document).ready(function () {
        if ('@TempData["returnMessage"]' != '')
            toastr.error('Aviso: @TempData["returnMessage"].');

        if ('@TempData["actionSuccessfully"]' != '')
            toastr.success('@TempData["actionSuccessfully"].');

        if (detectMobileDevice() == 0)
            activeSidebarMenu("@activeSideBar");

        $("h4").attr("style", "height:0px")
        $("h5").attr("style", "height:0px")
        $("code").attr("style", "height:1px;background-color:#fff;padding:2px 0px")

        $("input.gentelella-btn-success").on("click", function () {
            if ($("#txtComment").val() != "")
                $("#waiting").show();
        });

        activateRegistrationForm("CommentMatch", '@Model.actionUser');

        $("#tab_content2").find("td[target='scoreHome']").each(function (index, value) {
            var goalsH = parseInt($(this).attr("goalsHome"));
            var goalsA = parseInt($(this).attr("goalsAway"));
            var idHome = parseInt($(this).attr("idHome"));
            var idAway = parseInt($(this).attr("idAway"));
            var idTeam = parseInt('@Model.teamHomeID');
            if (goalsH == goalsA) { $(this).addClass("score-draw"); }
            else if (goalsH > goalsA && idTeam == idHome) { $(this).addClass("score-win"); }
            else if (goalsH > goalsA && idTeam == idAway) { $(this).addClass("score-lost"); }
            else if (goalsA > goalsH && idTeam == idAway) { $(this).addClass("score-win"); }
            else if (goalsA > goalsH && idTeam == idHome) { $(this).addClass("score-lost"); }
        });
        
        $("#tab_content2").find("td[target='scoreAway']").each(function (index, value) {
            var goalsH = parseInt($(this).attr("goalsHome"));
            var goalsA = parseInt($(this).attr("goalsAway"));
            var idHome = parseInt($(this).attr("idHome"));
            var idAway = parseInt($(this).attr("idAway"));
            var idTeam = parseInt('@Model.teamAwayID');
            if (goalsH == goalsA) { $(this).addClass("score-draw"); }
            else if (goalsH > goalsA && idTeam == idHome) { $(this).addClass("score-win"); }
            else if (goalsH > goalsA && idTeam == idAway) { $(this).addClass("score-lost"); }
            else if (goalsA > goalsH && idTeam == idAway) { $(this).addClass("score-win"); }
            else if (goalsA > goalsH && idTeam == idHome) { $(this).addClass("score-lost"); }
        });

        if ($("#listOfUserReceivingEmail").find("span[id='@userIDLogged']").length > 0)
            $("div.receiving-email").show();
        else
            $("div.dont-receive-email").show();
        
        objCalculateHistory = new calculateHistoryMatches({
            iTotalGoalsHomeTeam: @Model.historyCoachClash.totalGoalsUsuLogged,
            iTotalGoalsAwayTeam: @Model.historyCoachClash.totalGoalsUsuSearch,
            iTotalMatches: @(Model.historyCoachClash.totalWinUsuLogged + Model.historyCoachClash.totalWinUsuSearch + Model.historyCoachClash.totalDraw),
            iTotalWinsHomeTeam: @Model.historyCoachClash.totalWinUsuLogged,
            iTotalWinsAwayTeam: @Model.historyCoachClash.totalWinUsuSearch,
            iTotalDraws: @Model.historyCoachClash.totalDraw,
            sLabelTitleHometeam: "@Model.historyCoachClash.psnIDLogged",
            sLabelTitleAwayteam: "@Model.historyCoachClash.psnIDSearch"
        });
        objCalculateHistory.initializeHistory();

        objCalculateHistoryTeams = new calculateHistoryMatches({
            iTotalGoalsHomeTeam: @Model.historyTeamClash.totalGoalsTeamHome,
            iTotalGoalsAwayTeam: @Model.historyTeamClash.totalGoalsTeamAway,
            iTotalMatches: @(Model.historyTeamClash.totalWinTeamHome + Model.historyTeamClash.totalWinTeamAway + Model.historyTeamClash.totalDraw),
            iTotalWinsHomeTeam: @Model.historyTeamClash.totalWinTeamHome,
            iTotalWinsAwayTeam: @Model.historyTeamClash.totalWinTeamAway,
            iTotalDraws: @Model.historyTeamClash.totalDraw,
            sLabelTitleHometeam: "@Model.historyTeamClash.teamNameHome",
            sLabelTitleAwayteam: "@Model.historyTeamClash.teamNameAway",
            sNameIDLabelWinHomeTeam: "#idLabelWinHomeTeam_2",
            sNameIDLabelWinAwayTeam: "#idLabelWinAwayTeam_2",
            sNameIDWinsHomeTeam: "#winsHomeTeam_2",
            sNameIDWinsAwayTeam: "#winsAwayTeam_2",
            sObjectWinsHistoryMatchesHomeTeam: "#winsHistoryMatchesHomeTeam_2",
            sObjectWinsHistoryMatchesAwayTeam: "#winsHistoryMatchesAwayTeam_2",
            sObjectDrawsHistoryMatches: "#drawsHistoryMatches_2",
            sNameIDGoalsHomeTeamHistory: "#goalsHomeTeamHistory_2",
            sNameIDGoalsAwayTeamHistory: "#goalsAwayTeamHistory_2",
            sNameIDGoalsHomeTeam: "#goalsHomeTeam_2",
            sNameIDGoalsAwayTeam: "#goalsAwayTeam_2",
            sNameIDShowMatchesHomeTeam: "#gamesResultsCoachesHomeTeam_2",
            sNameIDShowMatchesAwayTeam: "#gamesResultsCoachesAwayTeam_2",
            sNameIDShowMatchesDraw: "#gamesResultsCoachesDraw_2"
        });
        objCalculateHistoryTeams.initializeHistory();
    });

    function hideShowTabs(id) {
        $("#tab_content1").hide();
        $("#tab_content2").hide();
        $("#tab_content3").hide();
        $("#tab_content4").hide();
        $(id).show();
    }

    function checkTextArea() {
        if ($("#txtComment").val() == "") {
            $("#txtComment").attr("style", "border:1px solid red;background-color:#F2DEDE");
        }
        else {
            $("#txtComment").attr("style", "");
        }
    }

    </script>
}

